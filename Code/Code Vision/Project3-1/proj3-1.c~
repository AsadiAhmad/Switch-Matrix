#include <mega32.h>
#include <delay.h>

void main(void){
    DDRA = 0b11111111;
    DDRB = 0b11111111;
    while (1){
        PORTA = 0b11111110;
        PORTB = 0b10111101; 
        PORTB = 0b11111111;
        PORTA = 0b00010001;
        PORTB = 0b11000011;
        PORTB = 0b11111111; 

    }
}
